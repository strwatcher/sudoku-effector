{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "component": {
    "default": {
      "path": "./src/widgets",
      "withStory": true,
      "withStyle": true,
      "withIndex": true,
      "withTest": false,
      "withLazy": false,
      "customTemplates": {
        "component": "./grc-templates/component/TemplateName.tsx",
        "style": "./grc-templates/component/style.module.scss",
        "index": "./grc-templates/component/index.ts",
        "story": "./grc-templates/component/TemplateName.stories.tsx"
      }
    },
    "page": {
      "path": "./src/pages",
      "withStory": true,
      "withStyle": false,
      "withTest": false,
      "withIndex": true,
      "withLazy": false,
      "customTemplates": {
        "component": "./grc-templates/page/TemplateName.tsx",
        "index": "./grc-templates/page/index.ts",
        "story": "./grc-templates/page/TemplateName.stories.tsx"
      }
    }
  },
  "testLibrary": "None"
}
